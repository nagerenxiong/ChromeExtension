{   
  "description": "网址收录器",     
  "content_scripts":[
      {
      "matches":["https://*.taobao.com/*","https://*.tmall.com/*"],
      "js":["jquery.js","content.js"]
      }
    ],  
  "icons":{               
    "48": "icon48.png"
  },
  "options_page": "options.html",
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
      "http://*/*"
  ],
  "manifest_version": 2,
   "name": "网址收录器",
   "version": "1"
}
